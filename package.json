{
  "name": "headstarter_pantry-tracker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run check-packages && next dev",
    "build": "npm run check-packages && next build",
    "start": "npm run check-packages && next start",
    "lint": "npm run check-packages && next lint",
    "export": "npm run check-packages && next export",
    "deploy:vercel": "npm run check-packages && vercel --prod",
    "deploy:firebase": "npm run check-packages && firebase deploy --only hosting",
    "deploy:gcloud": "npm run check-packages && gcloud run deploy pantry-tracker --image gcr.io/$PROJECT_ID/pantry-tracker:$COMMIT_SHA --platform managed --region $REGION --allow-unauthenticated"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "firebase": "^9.6.10",
    "next": "12.2.0",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "eslint": "8.15.0",
    "eslint-config-next": "12.2.0"
  }
}
